@model string

@{
    ViewData["Title"] = "Favorite Calendars";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
        <script src="~/Content/js/CalendarScripts.js" type="text/javascript"></script>
    </head>
    <body>
        @if (User.Identity.IsAuthenticated)
        {
            <h1 class="page-header">Favorite Calendars</h1>
            <br />

            <div class="main-content">
                @if (Model == "You do not have any favorite calendars")
                {
                    <h5>@Model</h5>
                }
                @*else
                {
                    @foreach (var calendar in Model)
                    {
                        <h5>@calendar.Name</h5>
                    }
                }*@
            </div>
        }
    </body>
</html>
