@{
    ViewData["Title"] = "Profiled Events List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script src="~/Content/js/HomeScripts.js" type="text/javascript"></script>
</head>
<body>
    @foreach (var individualEvent in Model)
    {
        <h5>@individualEvent.Description | @Html.ActionLink("Details", "Detail", "Event", new { eventId = individualEvent.Id })</h5>
    }
</body>
</html>

